{
    // See https://go.microsoft.com/fwlink/?LinkId=733558
    // for the documentation about the tasks.json format
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Make tiles",
            "type": "shell",
            "command": "node ./osm-tile-downloader/osm-tile-downloader.js --start-zoom-level 2 --end-zoom-level 6 --url https://b.tile.openstreetmap.org/{z}/{x}/{y}.png --output-dir ./tiles/ --delay 50 --yes"
        },
        {
            "label": "Install tile downloader",
            "type": "shell",
            "command": "git clone https://github.com/rogalmic/osm-tile-downloader.git ; cd osm-tile-downloader/ ; npm install"
        },
        
    ]
}